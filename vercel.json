{
  "installCommand": "pip install -r requirements.txt && python3.9 manage.py migrate",
  "buildCommand": "python3.9 manage.py collectstatic --noinput",
  "functions": {
    "bukoo/wsgi.py":{
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
        "src": "/(.*)",
        "dest": "bukoo/wsgi.py"
    }
],
  "outputDirectory": "static",
  "public": true
}